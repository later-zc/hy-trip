{
  "compilerOptions": {
    // jsconfig.json和tsconfig.js这两个文件vscode有做特殊处理，所以其中可以写注释
    // "target": "es5", 
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": true
  }
}
